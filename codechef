import java.util.*;
public class Prime{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int i,a,k=0;
System.out.println("Enter a number");
a=sc.nextInt();
for(i=1;i<a;i++){
if(a%i==0)
k+=1;
}
if(k==2)
System.out.println("Prime Number");
else
System.out.println("Not a Prime number");
}
}
